{
  "name": "quiz-app",
  "version": "1.0.0",
  "private": true,
  "description": "quizapp",
  "author": "Daniel Chu <cttrong98@juyou88.com>",
  "license": "ISC",
  "workspaces": [
    "apps/*",
    "libs/*"
  ],
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "prepare": "husky && husky install",
    "start:debug": "nest start --debug --watch",
    "start:prod-api-gateway": "node dist/apps/api-gateway/main.js",
    "start:prod-user-service": "node dist/apps/user-service/main.js",
    "start:prod-quiz-service": "node dist/apps/quiz-service/main.js",
    "start:prod-question-service": "node dist/apps/question-service/main.js",
    "start:prod-game-service": "node dist/apps/game-service/main.js",
    "start:prod-notification-service": "node dist/apps/notification-service/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/quiz-app/test/jest-e2e.json"
  },
